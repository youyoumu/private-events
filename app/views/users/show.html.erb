<%= link_to  "Index", events_path %>
<h1>My Events</h1>
<div>
  <ul>
    <% @my_events.each do |event| %>
      <li>
        <%= link_to event.name, event_path(event) %>
      </li>
    <% end %>
  </ul>
</div>
<h3>Attended events</h3>
<ul>
  <% current_user.attended_events.each do |event| %>
    <li> <%= link_to event.name, event_path(event) %> </li>
  <% end %>
</ul>